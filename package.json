{
  "name": "yarn",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:chanpreet3000/superdrug-order-automation.git",
  "author": "Chanpreet Singh <chanpreet3000@gmail.com>",
  "license": "MIT",
  "scripts": {
    "setup-backend": "cd backend && npx yarn install",
    "run-backend": "cd backend && node index.js",
    "setup-frontend": "cd frontend && npx yarn install",
    "run-frontend": "cd frontend && npx yarn start",
    "start": "concurrently \"yarn run-backend\" \"yarn run-frontend\""
  },
  "devDependencies": {
    "concurrently": "^9.0.1"
  }
}
